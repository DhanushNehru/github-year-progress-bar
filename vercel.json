{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/year-progress",
        "dest": "app.js"
      },
      {
        "src": "/",
        "dest": "app.js"
      }
    ],
    "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
  }